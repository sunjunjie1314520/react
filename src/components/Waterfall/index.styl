.wrap-image
    background #8413bd
    position relative
    float left
    width 100%
    height 850px
    overflow hidden
    .box
        float left
        width 100%
        transition all .5s cubic-bezier(0.22, 0.61, 0.36, 1)
        ul
            li
                float left
                position absolute
                img
                    width 100%
                    float left